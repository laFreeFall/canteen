    },
    "dependencies": {
        "bootstrap-vue": "^2.0.0-rc.11",
        "epic-spinners": "^1.0.3",
        "v-tooltip": "^2.0.0-rc.33",
        "vue-awesome": "^3.0.3",
        "vue-router": "^3.0.1",
        "vue-scrollto": "^2.11.0",
        "vue-snotify": "^3.2.0",
        "vue-wait": "^1.2.3",
        "vuedraggable": "^2.16.0",
        "vuex": "^3.0.1"
    }
